<!DOCTYPE html>
<html>
<head>
    
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - ZipShip</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top" style="background-color:purple">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>

                </button>
                
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><img src="~/Content/Images/logo_white.png" style="height:50px;width:70px;padding-top:5px;padding-right:20px" /></li>
                    <li><a href="/Home/Index" style="color:white"><b>Home</b></a></li>
                    <li><a href="/Home/About" style="color:white"><b>About</b></a></li>
                    <li><a href="/Home/Contact" style="color:white"><b>Contact</b></a></li>



                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()

        <hr />
        <footer style="color:white;font-weight:bolder">
            <p>&copy; Project By - 2016-CS-204,2016-CS-211,2016-CS-212,2016-CS-220 </p>
            <p> Submitted to - Sir Samyan Qayyum Wahla </p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
